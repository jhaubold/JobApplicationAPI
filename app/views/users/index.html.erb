<h1>Listing users</h1>

<table>
  <tr>
    <th>Token</th>
    <th>Email</th>
    <th>First name</th>
    <th>Last name</th>
    <th>Street</th>
    <th>Location code</th>
    <th>City</th>
    <th>Foto</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.token %></td>
    <td><%= user.email %></td>
    <td><%= user.first_name %></td>
    <td><%= user.last_name %></td>
    <td><%= user.street %></td>
    <td><%= user.location_code %></td>
    <td><%= user.city %></td>
    <td><%= user.foto %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>
